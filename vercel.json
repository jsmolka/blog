{
  "github": {
    "silent": true
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=86400, stale-while-revalidate=2592000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(woff|woff2)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.([0-9a-f]+)\\.(.*)",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
