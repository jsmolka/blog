# Cached for a day
/data/*
  Cache-Control: public
  Cache-Control: max-age=86400

# Cached for a week
/audio/*
  Cache-Control: public
  Cache-Control: max-age=604800

/img/*
  Cache-Control: public
  Cache-Control: max-age=604800

# Cached for a year
/css/*
  Cache-Control: public
  Cache-Control: max-age=31536000
  Cache-Control: immutable

/font/*
  Cache-Control: public
  Cache-Control: max-age=31536000
  Cache-Control: immutable

/js/*
  Cache-Control: public
  Cache-Control: max-age=31536000
  Cache-Control: immutable
